<p id="notice"><%= notice %></p>

<h1>Products</h1>



<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th>Installments</th>
      <th>Image</th>
      <th>Url</th>
      <th>Store</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.name %></td>
        <td><%= product.price %></td>
        <td><%= product.installments %></td>
        <td><%= product.image %></td>
        <td><%= product.url %></td>
        <td><%= product.store_id %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

